{
  "name": "react-redux-workshop",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "webpack",
    "test": "node_modules/.bin/jest",
    "lint": "node_modules/.bin/tslint 'src/**/*.ts' 'src/**/*.tsx'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "jest": "^15.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-test-renderer": "^15.3.2",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-mock-store": "^1.2.1",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.3.0",
    "typescript": "^2.0.5",
    "typings": "^1.3.3",
    "webpack": "^1.13.2"
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/components/*/**/*.{js,ts,tsx}"
    ],
    "modulePaths": [
      "<rootDir>/src/scripts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "scriptPreprocessor": "<rootDir>/web_modules/preprocessor.js",
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "devDependencies": {
    "tslint": "^3.15.1",
    "tslint-react": "^1.1.0"
  }
}
